@model ProyekPasti_20.ServiceKonfirmasiVaksin.Data_Pasien

@{
    ViewBag.Title = "insertLaporan";
    Layout = "~/Views/_LayoutPage1.cshtml";
}
<section id="home" class="w3l-banner py-5">
    <div class="banner-content">
        <div class="container pt-5 pb-md-4">
            <div class="row align-items-right">
                <div class="col-md-6 pt-md-0 pt-4">
                    <form method="post" action="@Url.Action("insertLaporan")">
                        <div class="one-frm">
                            ID Vaksin
                            <input type="text" name="id_vaksin" placeholder="ID Vaksin" class="form-control" required="" />
                        </div>
                        <div class="one-frm">
                            NIK Pasien
                            <input type="text" name="nik_pasien" placeholder="NIK" class="form-control" required="" />
                        </div>
                        <div class="one-frm">
                            No Rekam Medis Pasien
                            <input type="text" name="no_rekam_medis" placeholder="No Rekam Medis" class="form-control" required="" />
                        </div>
                        <div class="one-frm">
                            <input type="submit" value="Insert" class="btn btn-info" />
                            @Html.ActionLink("Back", "vaksinSampai", "User", new { @class = "btn btn-outline-info" })

                        </div>

                    </form>
                </div>

            </div>
        </div>
    </div>
</section>
